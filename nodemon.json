{
  "execMap": {
    "js": "babel-node"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": ".js,.jsx,.json",
  "ignore": [
    "test",
    "dist",
    "docs",
    "logs"
  ],
  "events": {
    "start": "sh nodemon.sh"
  },
  "script": "src/index.js",
  "restartable": "rs",
  "verbose": false,
  "watch": ["src"],
  "stdin": false,
  "stdout": true,
  "quiet": true,
  "colours": true
}
