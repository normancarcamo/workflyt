{
  "execMap": {
    "ts": "ts-node"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": ".ts,.tsx,.js,.jsx,.json",
  "ignore": [
    "test",
    "dist",
    "docs",
    "logs"
  ],
  "events": {
    "start": "sh nodemon.sh"
  },
  "script": "src/server.ts",
  "restartable": "rs",
  "verbose": false,
  "watch": ["src"],
  "stdin": false,
  "stdout": true,
  "quiet": true,
  "colours": true
}
