{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    [
      "module-resolver", {
        "cwd": "babelrc",
        "extensions": [ ".js", ".jsx", ".json" ],
        "root": [
          "./",
          "./logs",
          "./dist",
          "./docs",
          "./test",
          "./src"
        ]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        ["inline-dotenv", {
          "path": ".env.dev"
        }],
        "@babel/plugin-transform-runtime"
      ]
    },
    "test": {
      "plugins": [
        ["inline-dotenv", {
          "path": ".env.test"
        }],
        "@babel/plugin-transform-runtime"
      ]
    }
  }
}
